    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Current File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Launch generate-vyos-infterface-definition-structs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "tools/generate-vyos-infterface-definition-structs",
            "args": [
                "../../.build/interface-definitions/",
                "../../internal/vyos/vyosinterfaces",
                "vyosinterface"
            ]
        },
        {
            "name": "Launch generate-terraform-resource-full",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "tools/generate-terraform-resource-full/main.go",
            "args": [
                "../../internal/terraform/resource/autogen",
                "github.com/thomasfinstad/terraform-provider-vyos-rolling",
                "system/conntrack-timeout-custom-rule"
            ]
        }
    ]
}
